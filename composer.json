{
    "name": "cubes-doo/srbija-naselja",
    "description": "Spisak naselja, opstina, postanskih brojeva i okruga u Republici Srbiji",
    "type": "library",
    "authors": [
        {
            "name": "Aleksandar Dimic",
            "email": "aleksandar.dimic@cubes.rs"
        }
    ],
    "require": {
        "php": "^7 || ^8"
    },
    "require-dev": {
        "phpunit/phpunit": "^7 || ^8"
    },
    "autoload": {
        "psr-4": {
            "CubesDoo\\SrbijaNaselja\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CubesDoo\\SrbijaNaselja\\Laravel\\ServiceProvider"
            ],
            "aliases": {
                "SrbijaNaselja": "CubesDoo\\SrbijaNaselja\\Laravel\\Facade"
            }
        }
    }
    
}
